<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:FPD.UI.Windows.Security"
                    xmlns:cview="clr-namespace:FPD.UI.Windows"
                    xmlns:aview="clr-namespace:FPD.UI.Windows.Asset"
                    xmlns:vm="clr-namespace:FPD.Logic.ViewModels.Security;assembly=FPD.Logic"
                    xmlns:vma="clr-namespace:FPD.Logic.ViewModels.Asset;assembly=FPD.Logic"
                    xmlns:cvm="clr-namespace:FPD.Logic.ViewModels.Common;assembly=FPD.Logic"
                    xmlns:svm="clr-namespace:FPD.Logic.ViewModels.Stats;assembly=FPD.Logic">

    <DataTemplate DataType="{x:Type vm:SelectedSecurityViewModel}">
        <view:SelectedSecurityView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvm:DataNamesViewModel}">
        <cview:DataNamesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vma:SelectedAssetViewModel}">
        <aview:SelectedAssetView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvm:SelectedSingleDataViewModel}">
        <cview:SelectedSingleDataView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvm:TimeListViewModel}">
        <cview:TimeListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type svm:AccountStatsViewModel}">
        <cview:AccountStatsControl />
    </DataTemplate>
</ResourceDictionary>
