<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:FinanceWindows.StatsWindow"
                    xmlns:vm="clr-namespace:FinanceViewModels.StatsViewModels">

    <DataTemplate DataType="{x:Type vm:MainTabViewModel}">
        <view:StatsWindowMainTab/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SecuritiesStatisticsViewModel}">
        <view:SecuritiesStatistics/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SecurityInvestmentViewModel}">
        <view:SecurityInvestments/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:BankAccStatsViewModel}">
        <view:BankAccountStats/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:HtmlStatsViewerViewModel}">
        <view:HtmlStatsViewer/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:DataBaseStatsViewModel}">
        <view:DataBaseStats/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:PortfolioHistoryViewModel}">
        <view:PortfolioHistory/>
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type vm:StatisticsChartsViewModel}">
        <view:StatisticsCharts/>
    </DataTemplate>
</ResourceDictionary>