<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:FinanceWindows"
                    xmlns:vm="clr-namespace:FinanceWindowsViewModels"
                    xmlns:cvm="clr-namespace:FinanceCommonViewModels">
    <DataTemplate DataType="{x:Type vm:BasicDataViewModel}">
        <view:BasicDataView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SecurityEditWindowViewModel}">
        <view:SecurityEditWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type cvm:SingleValueEditWindowViewModel}">
        <view:SingleValueEditWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StatsCreatorWindowViewModel}">
        <view:StatsCreatorWindow />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ReportingWindowViewModel}">
        <view:ReportingWindow />
    </DataTemplate>
</ResourceDictionary>