<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:FinancePortfolioDatabase.GUI.Windows.Security"
                    xmlns:cview="clr-namespace:FinancePortfolioDatabase.GUI.Windows.Common"
                    xmlns:vm="clr-namespace:FinancePortfolioDatabase.GUI.ViewModels.Security"
                    xmlns:cvm="clr-namespace:FinancePortfolioDatabase.GUI.ViewModels.Common">
    
    <DataTemplate DataType="{x:Type vm:SelectedSecurityViewModel}">
        <view:SelectedSecurityView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvm:DataNamesViewModel}">
        <cview:DataNamesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvm:SelectedSingleDataViewModel}">
        <cview:SelectedSingleDataView/>
    </DataTemplate>
</ResourceDictionary>