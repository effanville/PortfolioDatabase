<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Effanville.FPD.Logic.ViewModels.Security;assembly=Effanville.FPD.Logic"
                    xmlns:cvm="clr-namespace:Effanville.FPD.Logic.ViewModels.Common;assembly=Effanville.FPD.Logic"
                    xmlns:vma="clr-namespace:Effanville.FPD.Logic.ViewModels.Asset;assembly=Effanville.FPD.Logic"
                    xmlns:svm="clr-namespace:Effanville.FPD.Logic.ViewModels.Stats;assembly=Effanville.FPD.Logic"
                    xmlns:security="clr-namespace:Effanville.FPD.UI.Windows.Security"
                    xmlns:stats="clr-namespace:Effanville.FPD.UI.Windows.Stats"
                    xmlns:common="clr-namespace:Effanville.FPD.UI.Windows.Common"
                    xmlns:asset="clr-namespace:Effanville.FPD.UI.Windows.Asset">

    <DataTemplate DataType="{x:Type vm:SelectedSecurityViewModel}">
        <security:SelectedSecurityView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvm:DataNamesViewModel}">
        <common:DataNamesView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vma:SelectedAssetViewModel}">
        <asset:SelectedAssetView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvm:SelectedSingleDataViewModel}">
        <common:SelectedSingleDataView/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type cvm:TimeListViewModel}">
        <common:TimeListView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type svm:AccountStatsViewModel}">
        <stats:AccountStatsControl />
    </DataTemplate>
    <DataTemplate DataType="{x:Type svm:ExportDataViewModel}">
        <stats:ExportDataView />
    </DataTemplate>
</ResourceDictionary>
